<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.web.rizki.apotikku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Daftar" />
        <activity
            android:name=".ListObat"
            android:parentActivityName=".Navigasi">
            <intent-filter>
                <action android:name="com.id.web.rizki.apotik"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".DetailObat" />
        <activity android:name=".Cart" />
        <activity android:name=".Menu" />
        <activity
            android:name=".Navigasi"
            android:label="@string/title_activity_navigasi"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>